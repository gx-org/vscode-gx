{
	"name": "gx",
	"readme": "README.md",
	"displayName": "GX",
	"description": "GX language support for Visual Studio Code",
	"icon": "icons/icon.png",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "*"
	},
	"publisher": "Google",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"grammars": [
			{
				"language": "gx",
				"path": "syntaxes/gx.tmLanguage.json",
				"scopeName": "source.gx"
			}
		],
		"languages": [
			{
				"id": "gx",
				"aliases": [
					"GX"
				],
				"configuration": "language-configuration.json",
				"extensions": [
					".gx"
				],
				"icon": {
					"light": "icons/icon.png",
					"dark": "icons/icon.png"
				}
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gx-org/vscode-gx.git"
	}
}
